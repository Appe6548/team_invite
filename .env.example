# 管理员密码（必须修改）
ADMIN_PASSWORD=admin123

# 管理员 TOTP 密钥（可选，Base32 格式，如 JBSWY3DPEHPK3PXP）
# 配置后登录需要输入 Google Authenticator 等 App 生成的验证码
ADMIN_TOTP_SECRET=

# Session 密钥（可选，不设置会自动生成）
SECRET_KEY=your-random-secret-key

# JWT 密钥（可选，不设置会自动生成）
JWT_SECRET=your-jwt-secret

# 数据库路径（可选，默认 data.db）
DB_PATH=data.db

# 端口（可选，默认 5000）
PORT=5000

# 调试模式（可选）
DEBUG=false

# 自动同步间隔（秒，默认30）
SYNC_INTERVAL=30
